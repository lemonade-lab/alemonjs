  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.4.1",
