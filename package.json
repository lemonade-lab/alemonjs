    "lodash-es": "^4.17.21",
    "lru-cache": "^10.2.0",
    "memory-cache": "^0.2.0",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.12",
    "protobufjs": "^7.2.5",
    "public-ip": "^6.0.1",
    "puppeteer": "^20.7.1",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/koa": "^2.13.11",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.7",
    "@types/koa2-cors": "^2.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/memory-cache": "^0.2.5",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.2.4",
    "@types/node-schedule": "^2.1.2",
    "@types/nodemailer": "^6.4.14",
    "@types/progress": "^2.0.5",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.5",
    "husky": "^9.0.11",
    "prettier": "^2.8.8",
    "rollup": "^4.16.4",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "types": "dist",
  "files": [
    ".puppeteerrc.cjs",
    "env.d.ts",
    "dist",
    "config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./.puppeteerrc.cjs": {
      "require": "./.puppeteerrc.cjs"
    },
    "./env": {
      "types": "./env.d.ts"
    },
    "./tsconfig.json": {
      "import": "./config/tsconfig.json",
      "require": "./config/tsconfig.json"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "bugs": "https://github.com/ningmengchongshui/alemonjs/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ningmengchongshui/alemonjs.git"
  },
  "homepage": "https://alemonjs.com/"
}
