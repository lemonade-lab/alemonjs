{
  "name": "alemonjs",
  "version": "1.2.1-rc.0",
  "description": "DOCS https://alemonjs.com/",
  "scripts": {
    "test": "npx alemonjs",
    "dev": "ts-node alemon.config.ts",
    "build:bin": "tsc --module NodeNext  ./bin/index.ts",
    "build:min.d": "rollup --config version/min.config.d.js",
    "build": "rollup --config version/rollup.config.js",
    "build:min": "rollup --config version/min.config.js",
    "build:run": "rollup --config version/run.config.js",
    "push": "npm run build  && npm run build:min && npm run build:min.d && npm run build:run && npm run build:bin",
    "lint": "commitlint --config .commitlint.config.cjs -e -V",
    "doc": "typedoc --options typedoc.json",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.4.1",
    "file-type": "^18.5.0",
    "form-data": "^4.0.0",
    "image-size": "^1.0.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^7.0.0",
    "koa2-cors": "^2.0.6",
    "lodash-es": "^4.17.21",
    "lru-cache": "^10.2.0",
    "memory-cache": "^0.2.0",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.12",
    "protobufjs": "^7.2.5",
    "public-ip": "^6.0.1",
    "puppeteer": "^20.7.1",
    "qrcode": "^1.5.3",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/koa": "^2.13.11",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.7",
    "@types/koa-static": "^4.0.4",
    "@types/koa-websocket": "^5.0.10",
    "@types/koa2-cors": "^2.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/memory-cache": "^0.2.5",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.2.4",
    "@types/node-schedule": "^2.1.2",
    "@types/nodemailer": "^6.4.14",
    "@types/progress": "^2.0.5",
    "@types/qrcode": "^1.5.0",
    "@types/ws": "^8.5.5",
    "husky": "^9.0.11",
    "prettier": "^2.8.8",
    "rollup": "^4.16.4",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "author": "ningmengchongshui",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    ".puppeteerrc.cjs",
    "min.js",
    "min.d.ts"
  ],
  "main": "dist/index.js",
  "types": "dist",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./.puppeteerrc.cjs": {
      "require": "./.puppeteerrc.cjs"
    },
    "./min.js": {
      "require": "./min.js",
      "types": "./min.d.ts"
    },
    "./bin/index.js": {
      "require": "./bin/index.js"
    }
  },
  "bin": {
    "alemonjs": "./bin/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
